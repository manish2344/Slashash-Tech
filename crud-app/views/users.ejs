<!-- views/users.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Users List</title>
  <link rel="stylesheet" href="/style.css">
</head>
<body>
  <h1>Users List</h1>
  <ul>
    <% users.forEach(user => { %>
      <li><strong><%= user.name %></strong> - <%= user.email %></li>
    <% }) %>
  </ul>
  <a href="/">Add New User</a>
</body>
</html>
